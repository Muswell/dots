# Increase Volume
"pactl set-sink-mute @DEFAULT_SINK@ false ; pactl set-sink-volume 0 +5%"
    XF86AudioRaiseVolume

#Decrease Volume
"pactl set-sink-mute @DEFAULT_SINK@ false ; pactl set-sink-volume 0 -5%"
    XF86AudioLowerVolume

#Mute Volume
"pactl set-sink-mute @DEFAULT_SINK@ toggle"
    XF86AudioMute

# Play/Pause
"mpc toggle"
    XF86AudioPlay

# Previous Song
"mpc prev"
    XF86AudioPrev

# Next Song
"mpc next"
    XF86AudioNext
